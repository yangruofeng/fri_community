/*client*/
.client-detail-table label {
  margin-bottom: 0;
}
.client-detail-table tr:nth-child(even) {
  background-color: #fff!important;
}
.client-detail-table tr td:nth-child(odd) {
  background-color: #f3f3f3;
}
.client-detail-table>tbody>tr>td {
  vertical-align: middle;
}
.avatar-lg {
  width: 120px;
}
.avatar-sm {
  width: 80px;
  vertical-align: bottom;
  margin-left: 10px;
}
.verify-status {
  font-size: 12px;
  margin-left: 5px;
  color: #f00;
}
.verify-status.alr {
  color: #32BC61;
}

.client-detail-wrap {

}
.client-detail-wrap .left-wrap {
  width: 40%;
  float: left;
}
.client-detail-wrap .client-info {
  background-color: #fff;
  padding: 20px 0 0;
}
.client-detail-wrap .client-info .avatar-wrap {
  position: relative;
  height: 120px;
  border-bottom: 1px solid #e7eaec;
}
.client-detail-wrap .client-info .avatar-wrap img {
  position: absolute;
  top: 0;
  left: 20px;
  width: 100px;
  height: 100px;
  border-radius: 50%;
}
.client-detail-wrap .client-info .avatar-wrap > .info {
  padding-left: 135px;
  padding-top: 10px;
}
.client-detail-wrap .client-info .avatar-wrap > .info p {
  margin-bottom: 0;
}
.client-detail-wrap .client-info .avatar-wrap > .info > .name {
  font-size: 18px;
  font-weight: 600;
}
.client-detail-wrap .client-info .credit-info {
  height: 100px;
}
.client-detail-wrap .client-info .credit-info .item {
  width: 33.33333%;
  text-align: center;
  float: left;
  padding: 30px 0;
}
.client-detail-wrap .client-info .credit-info .arrears {
  border-left: 1px solid #e7eaec;
  border-right: 1px solid #e7eaec;
}
.client-detail-wrap .client-info .credit-info .item p {
  margin-bottom: 0;
  font-size: 20px;
  margin-top: 5px;
  color: #f60;
}
.client-detail-wrap .member-info {
  background-color: #fff;
  margin-top: 10px;
}
.client-detail-wrap .contract-wrap {
  width: 100%;
  margin-top: 10px;
}
.client-detail-wrap .contract-list {
  background-color: #fff;
}
.pull-right {
    float: right;
}
.text-muted {
    color: #888888;
}
.client-detail-wrap .verify-wrap {
  width: 100%;
}
.client-verify-info {
  background-color: #fff;
  border: 1px solid #ddd;
  border-top: 0;
}
.client-verify-info .item {
  margin-top: 0;
  padding: 15px;
}
.client-verify-info .item span {
  font-size: 12px;
  margin-left: 5px;
  float: right;
}
.client-verify-info .item span.checked {
  color: #32BC61;
}
.client-verify-info .item span i {
  margin-right: 5px;
}
.wrap-right {
  width: 59%;
  float: right;
}
/*contract*/
.important-info .item {
  background-color: #fff;
  text-align: center;
  float: left;
  width: 23.5%;
  margin-right: 2%;

}
.important-info .item:last-child {
  margin-right: 0;
}
.important-info .item p {
  margin-bottom: 0;
  font-size: 14px;
  font-weight: 600;
  text-align: left;
  padding: 10px 10px 7px;
  border-bottom: 1px solid #e7eaec;
}
.important-info .item .c {
  font-size: 18px;
  padding: 20px 0;
}
.base-info {
  margin-top: 15px;
}
.base-info .content {
  padding: 0 15px;
}
.base-info .content .wrap {
  width: 50%;
  float: left;
}
.base-info label {
  font-weight: 100;
}
.base-info table {
  font-size: 12px;
}
.base-info table td {
  padding: 5px;
}
.contract-base-info {
  float: left;
  width: 68%;
  background-color: #fff;
}
.contract-base-info .content {
  position: relative;
}
.contract-base-info .content.fold {
  overflow: hidden;
}
.contract-base-info .content .displayall {
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  color: #d40606;
  border-top: 1px solid #e7eaec;
  filter: alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=red,endcolorstr=blue,gradientType=0);
  -ms-filter: alpha(opacity=100 finishopacity=50 style=1 startx=0,starty=0,finishx=0,finishy=150) progid:DXImageTransform.Microsoft.gradient(startcolorstr=red,endcolorstr=blue,gradientType=0);
  background: #ddd;
  background: -moz-linear-gradient(top, #ddd, rgba(255, 255, 255, 0.7));
  background: -webkit-gradient(linear, 0 0, 0 bottom, from(#fff), to(rgba(231, 234, 236, 0.7)));
  background: -o-linear-gradient(top, #ddd, rgba(255, 255, 255, 0.7));
}
.contract-base-info .content .displayall i {
  margin-left: 5px;
  font-size: 18px;
}
.contract-base-info .content .displayall:hover {
  cursor: pointer;
}
.contract-base-info .hiddenall {
  width: 100%;
  height: 35px;
  line-height: 35px;
  text-align: center;
  color: #d40606;
  border-top: 1px solid #e7eaec;
  border-bottom: 1px solid #e7eaec;
  background-color: #f2f2f2;
}
.contract-base-info .hiddenall i {
  margin-left: 5px;
  font-size: 18px;
}
.contract-base-info .hiddenall:hover {
  cursor: pointer;
}
.product-info {
  float: right;
  width: 30%;
  background-color: #fff;
}


.contract-plan {
  margin-top: 15px;
}
.contract-plan .ibox-title h5 {
  margin-top: 5px;
}
.base-info .ibox-title {
  border-bottom: 1px solid #e7eaec;
  min-height: 40px;
}
.base-info .ibox-title h5 {
  margin-bottom: 0;
}
.loan-plan {
  background-color: #fff;
  width: 49%;
  float: left;
}
.contract-plan .item {
  padding: 15px 20px 15px 70px;
  margin-top: 0;
  position: relative;
  overflow: hidden;
}
.contract-plan .item .state-img {
  background-image: url(../image/contract-state.png);
  background-repeat: no-repeat;
  background-position: -100px -390px;
  overflow: hidden;
  position: absolute;
  right: -10px;
  bottom: -10px;
  height: 80px;
  width: 80px;
}
.loan-plan .item .loan-done {
  background-position: -10px -8px;
  display: block;
}
.loan-plan .item .loan-going {
  background-position: -10px -390px;
  display: block;
}
.contract-plan .item .period {
  position: absolute;
  top: 24px;
  left: 20px;
  width: 40px;
  height: 40px;
  line-height: 40px;
  color: #333;
  background: #e6e6e6;
  text-align: center;
  font-size: 18px;
  border-radius: 50%;
}
.contract-plan .ibox-content {
  padding: 0;
}
.loan-amount {
  color: #333;
  margin-bottom: 6px;
  float: left;
}
.loan-amount em {
  font-size: 20px;
  font-style: normal;
  color: #ea544a;
}

.loan-exp {
  float: left;
  margin-left: 5px;
  position: relative;
  margin-top: 9px;
}
.loan-exp > span {
  color: #5b9fe2;
}
.loan-exp > span:hover {
  color: #ea544a;
}
.loan-exp-wrap {
  filter: alpha(Opacity=0);
  opacity: 0;
  -moz-transition: top .2s ease-in-out,opacity .2s ease-in-out;
  -o-transition: top .2s ease-in-out,opacity .2s ease-in-out;
  -webkit-transition: top .2s ease-in-out,opacity .2s ease-in-out;
  transition: top .2s ease-in-out,opacity .2s ease-in-out;
  visibility: hidden;
  position: absolute;
  top: 24px;
  left: -35px;
  padding: 7px 10px;
  border: 1px solid #ddd;
  background-color: #f6fcff;
  color: #5b9fe2;
  font-size: 12px;
  font-family: Arial,"Hiragino Sans GB",simsun;
}
.loan-exp-hover .loan-exp-wrap {
  filter: alpha(enabled=false);
  opacity: 1;
  visibility: visible;
}
.loan-exp-wrap .pos {
  position: relative;
}
.loan-exp-table .t {
  color: #a5a5a5;
  font-size: 12px;
  font-weight: 400;
  text-align: left;
}
.loan-exp-table .a {
  color: #000;
  font-size: 18px;
  font-weight: 400;
  text-align: left;
}
.loan-exp-table .a .y {
  color: #ea544a;
}
.loan-date {
  color: #a5a5a5;
}
.repayment-plan {
  background-color: #fff;
  width: 49%;
  float: right;
}
.repayment-plan .item .repayment-done {
  background-position: -10px -95px;
  display: block;
}
.repayment-plan .item .repayment-going {
  background-position: -10px -290px;
  display: block;
}
.repayment-plan .item .repayment-overdue {
  background-position: -10px -190px;
  display: block;
}
